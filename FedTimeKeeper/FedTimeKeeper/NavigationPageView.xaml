<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FedTimeKeeper.NavigationPageView">

    <StackLayout>
        <Button Text="Home Page" Margin="5" x:Name="HomePageButton" Clicked="HomePageButton_Clicked"></Button>
        <Button Text="View Leave Information" Margin="5" x:Name="ViewLeaveButton" Clicked="ViewLeaveButton_Clicked"></Button>
        <Button Text="Scheduled Leave" Margin="5" x:Name="ScheduledLeaveButton" Clicked="ScheduledLeaveButton_Clicked"></Button>
        <Button Text="Add Leave" Margin="5" x:Name="AddLeaveButton" Clicked="AddLeaveButton_Clicked"></Button>
    </StackLayout>
</ContentPage>